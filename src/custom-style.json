{
    "sources": {
    	"output":{
    		"type": "geojson",
    		"data": {
    			"type": "FeatureCollection",
    			"features": []
    		}
    	
    	}
    },



    "layers": [		

    {
            "id": "neighborhoods-outline",
            "type": "line",
            "source": "output",
            "paint": {
                "line-color": "rgb(94, 164, 198)",
                "line-opacity": 0.7,
                "line-width": 3
            }
        },								

    	{
    		"id": "output",
    		"type": "fill-extrusion",
    		"source": "output",
    		"paint": {
    			"fill-extrusion-height": ["*",["get","count"],5],
                "fill-extrusion-color": {
                    "property": "count",
                    "stops": [
                        [0,"#f2f0f7"],
                        [30,"#dadaeb"],
                        [100,"#bcbddc"],
                        [300,"#9e9ac8"],
                        [600,"#807dba"],
                        [800,"#6a51a3"],
                        [1200,"#4a1486"]
                    ]
                },
                "fill-extrusion-opacity": 0.9

    		}

    	}
    	]
}
